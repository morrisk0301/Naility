<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <div class="">
            <div class="page-title">
                <div class="title_left">
                    <h3> 시술 검색 </h3>
                </div>

            </div>
            <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <div class="col-md-9 col-sm-5 col-xs-12 form-group top_search">
                                <div class="input-group">
                                    <input type="text" class="form-control" id="search_text" placeholder="시술 명을 입력하세요">
                                    <span class="input-group-btn">
                                    <button class="btn btn-default" id="btn_search" type="button">검색 </button>
                                </span>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>

                        <div class="x_content">
                            <div class="table-responsive">
                                <table class="table table-striped jambo_table bulk_action">
                                    <thead>
                                    <tr class="headings">
                                        <th class="column-title">선택 </th>
                                        <th class="column-title">번호 </th>
                                        <th class="column-title">시술 명 </th>
                                        <th class="column-title">시술 카테고리</th>
                                        <th class="column-title">시술 가격 </th>
                                        <th class="column-title">생성일 </th>
                                        </th>
                                        <th class="bulk-actions" colspan="7">
                                            <a class="antoo" style="color:#fff; font-weight:500;">시술 선택 <i class="fa fa-chevron-down"></i></a>
                                        </th>
                                    </tr>
                                    </thead>

                                    <tbody>
                                    <%for(var i=0;i<procedure.length;i++){
                                    if(i%2===0){%>
                                    <tr class="even pointer">
                                        <td class="a-center">
                                            <input type="checkbox" class="flat checkbox_procedure" name="table_records" id="<%=procedure[i].procedure_id%>">
                                        </td>
                                        <td class=" "><%=(parseInt(page)-1)*15+i+1%></td>
                                        <td class=" " id="name_<%=procedure[i].procedure_id%>"><%=procedure[i].procedure_name%></td>
                                        <td class=" "><%=procedure[i].procedure_category%></td>
                                        <td class=" " id="price_<%=procedure[i].procedure_id%>"><%=procedure[i].procedure_price%></td>
                                        <td class="a-right a-right "><%=procedure[i].created_at.getFullYear() + '년 '+ (procedure[i].created_at.getMonth() + 1).toString() + '월 ' + procedure[i].created_at.getDate() + '일 '%></td>
                                        </td>
                                    </tr>
                                    <%}else{%>
                                    <tr class="odd pointer">
                                        <td class="a-center ">
                                            <input type="checkbox" class="flat checkbox_procedure" name="table_records" id="<%=procedure[i].procedure_id%>">
                                        </td>
                                        <td class=" "><%=(parseInt(page)-1)*15+i+1%></td>
                                        <td class=" " id="name_<%=procedure[i].procedure_id%>"><%=procedure[i].procedure_name%></td>
                                        <td class=" "><%=procedure[i].procedure_category%></td>
                                        <td class=" " id="price_<%=procedure[i].procedure_id%>"><%=procedure[i].procedure_price%></td>
                                        <td class="a-right a-right "><%=procedure[i].created_at.getFullYear() + '년 '+ (procedure[i].created_at.getMonth() + 1).toString() + '월 ' + procedure[i].created_at.getDate() + '일 '%></td>
                                        </td>
                                    </tr>
                                    <%}}%>
                                    </tbody>
                                </table>
                                <button class="btn btn-dark alignright" id="btn_procedure">시술 선택 </button>
                                <div class="text-center"id="page-selection"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

