<div id="CalenderModalNew" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myModalLabel">예약 추가</h4>
            </div>
            <div class="modal-body">
                <div id="testmodal" style="padding: 5px 20px;">
                    <form data-parsley-validate class="form-horizontal form-label-left">
                        <div class="form-group">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12">회원 검색<span class="required">*</span></label>
                            <div class="col-md-9 col-sm-9 col-xs-12">
                                <div class='input-group'>
                                    <input type="text" class="form-control" name="name" id="ap_name" placeholder="회원 이름을 입력하세요 " >
                                    <span class="input-group-btn">
                                        <button id="btn_search_member" type="button" class="btn btn-primary">검색 </button>
                                        <button id="btn_add" type="button" class="btn btn-primary">추가 </button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12">시술 검색<span class="required">*</span></label>
                            <div class="col-md-9 col-sm-9 col-xs-12">
                                <div class='input-group'>
                                    <input type="text" class="form-control" name="procedure" id="ap_procedure"placeholder="시술 이름을 입력하세요 ">
                                    <span class="input-group-btn">
                                        <button id="btn_search_procedure" type="button" class="btn btn-primary">검색 </button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12">예약 시간</label>
                            <div class="col-md-9 col-sm-9 col-xs-12">
                                <div class="form-control" id="date">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12">종료 시간</label>
                            <div class="col-md-9 col-sm-9 col-xs-12">
                                <div class="form-control" id="date_end">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3 col-sm-3 col-xs-12">가격</label>
                            <div class="col-md-9 col-sm-9 col-xs-12">
                                <div class="form-control" id="ap_price">
                                </div>
                            </div>
                        </div>
                        <div class="ln_solid"></div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="submit" id="btn_appointment_new" class="btn btn-primary antosubmit">추가 </button>
                <button type="button" class="btn btn-default antoclose" data-dismiss="modal">닫기 </button>
            </div>
        </div>
    </div>
</div>
<div id="CalenderModalEdit" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">

