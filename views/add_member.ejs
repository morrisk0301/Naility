<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <div class="">
            <div class="page-title">
                <div class="title_left">
                    <h3> 회원 추가 </h3>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>회원 정보</h2>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <br/>
                            <%if(!modify){%>
                                <form id="form_member" data-parsley-validate class="form-horizontal form-label-left" action="/member" method="post">
                                    <div class="form-group">
                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">회원 이름 </label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <input type="text" id="member_name" required="required" name="name" class="form-control col-md-7 col-xs-12">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="middle-name" class="control-label col-md-3 col-sm-3 col-xs-12">휴대폰 번호 </label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <input id="member_phone" class="form-control col-md-7 col-xs-12" name="phone" required="required" type="text">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="middle-name" class="control-label col-md-3 col-sm-3 col-xs-12">연락 경로 </label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <select id="member_contact" class="form-control">
                                                <option>연락 경로를 선택하세요</option>
                                                <option>전화</option>
                                                <option>문자</option>
                                                <option>카카오톡</option>
                                                <option>인스타그램</option>
                                                <option>페이스북</option>
                                                <option>로드</option>
                                                <option>지인</option>
                                                <option>기타</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="ln_solid"></div>
                                    <div class="form-group">
                                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                            <button type="submit" class="btn btn-success">추가하기 </button>
                                        </div>
                                    </div>

                                </form>
                            <%} else{%>
                            <form id="form_member2" data-parsley-validate class="form-horizontal form-label-left">
                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12">회원 이름 </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input type="text" id="member_name2" required="required" name="name" class="form-control col-md-7 col-xs-12" value="<%=member.member_name%>">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="middle-name" class="control-label col-md-3 col-sm-3 col-xs-12">휴대폰 번호 </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <input id="member_phone2" class="form-control col-md-7 col-xs-12" name="phone" required="required" type="text" value="<%=member.member_phone%>">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="middle-name" class="control-label col-md-3 col-sm-3 col-xs-12">연락 경로 </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        <select id="member_contact2" class="form-control">
                                            <option><%=member.member_contact%></option>
                                        </select>
                                    </div>
                                </div>
                                <div class="ln_solid"></div>
                                <div class="form-group">
                                    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                        <button type="submit" class="btn btn-success">수정하기 </button>
                                    </div>
                                </div>

                            </form>
                            <%}%>

                        </div>
                    </div>
                </div>
            </div>
        </div>
